  "version": "0.2.0",
  "configurations": [
    {
      "name": "do_ai (dev)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--debug",
        "-v",
        "--dart-define-from-file",
        "envs/dev.env"
        // "--web-experimental-hot-reload"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "do_ai (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "do_ai (release mode)",
      "request": "launch",
      "type": "dart",
      "args": ["--debug", "-v", "--dart-define-from-file", "envs/prod.env"],
      "flutterMode": "release"
    }
  ]
}
