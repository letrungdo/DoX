{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "do_ai (dev)",
      "request": "launch",
      "type": "dart",
      "preLaunchTask": "restore-env-configs",
      "args": [
        "--debug",
        "-v",
        "--dart-define-from-file",
        "envs/dev/dart-define.env",
        "--web-browser-flag",
        "--disable-web-security"
      ],
      "program": "lib/main.dart"
    },
    {
      "name": "do_ai (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "do_ai (release mode)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--debug",
        "-v",
        "--dart-define-from-file",
        "envs/dev/dart-define.env"
      ],
      "flutterMode": "release"
    }
  ]
}
